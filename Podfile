# Uncomment the next line to define a global platform for your project
# platform :ios, ’10.3’

target 'SwiftGoogleMap' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for SwiftGoogleMap
    source 'https://github.com/CocoaPods/Specs.git'
    pod 'GoogleMaps'
    pod 'GooglePlaces'
    pod 'RxSwift',    '~> 3.2’
    pod 'RxCocoa',    '~> 3.0'
    pod 'Alamofire', '~> 4.4'
    pod 'SwiftyJSON'

  target 'SwiftGoogleMapTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'RxBlocking', '~> 3.0'
    pod 'RxTest',     '~> 3.0'
  end

  target 'SwiftGoogleMapUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
